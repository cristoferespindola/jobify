<%- include('header') %>

<body>
  <div class="row">
    <div class="title">
      <h2 style="display: inline-block;">Vagas em aberto:</h2> &nbsp;
      <% if(dev) { %>
      <a href="/admin/">Admin</a>
      <% } %>
    </div>
    <% categorias.forEach( categoria => {  %>
    <h3> <%- categoria.nome %> </h3>
    <ul>
      <% categoria.vagas.forEach( vaga => { %>
      <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
      <% }) %>
    </ul>
    <% }) %>
  </div>


  <%- include('footer') %>