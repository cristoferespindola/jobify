<%- include('header') %>
  <body>
    <div class="row">
      <h2 class="title">Open roles:</h2>
      <% categorias.forEach( categoria => {  %>
        <h3> <%- categoria.name %> </h3>
        <ul>
          <% categoria.vagas.forEach( vaga => { %>
            <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
          <% }) %>
        </ul>
        <% }) %>
    </div>


<%- include('footer') %>

