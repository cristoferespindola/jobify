<%- include('../../header') %>

<body>
  <div class="row">
    <div class="title">
      <h2 style="display: inline-block;">Administrar vagas:</h2> &nbsp;
      <a href="/admin/vagas/nova">Nova vaga</a> &nbsp;
      <a href="/admin" style="font-size: 18px; ">Voltar</a>
    </div>
    <ul>
      <% vagas.forEach( vaga => {  %>
      <li>
        <%- vaga.titulo %> |
        <a href="/admin/vagas/deletar/<%- vaga.id %>">Excluir</a>
        <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
      </li>
      <% }) %>
    </ul>
  </div>


  <%- include('../../footer') %>